<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/scrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="false">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context="com.jianqingc.nectar.fragment.Network_Fragment.SubnetDetailFragment">

        <LinearLayout
            android:id="@+id/PortDetail1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:orientation="horizontal"

            android:weightSum="1">

            <TextView
                android:id="@+id/portNameID1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/textView"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="20sp"
                android:layout_weight="0.3"
                android:text="Port Name"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/portNameID"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20sp"
                android:layout_weight="0.7"
                android:text="Medium Text"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textIsSelectable="true" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/PortDetail2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/PortDetail1"
            android:layout_centerHorizontal="true"
            android:orientation="horizontal"

            android:weightSum="1">

            <TextView
                android:id="@+id/portID1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="20sp"
                android:layout_weight="0.3"
                android:text="ID"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/portID"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20sp"
                android:layout_weight="0.7"
                android:text="Medium Text"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textIsSelectable="true" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/PortDetail3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/PortDetail2"
            android:layout_centerHorizontal="true"
            android:orientation="horizontal"

            android:weightSum="1">

            <TextView
                android:id="@+id/portNetworkID1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="20sp"
                android:layout_weight="0.3"
                android:text="NetworkID"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/portNetworkID"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20sp"
                android:layout_weight="0.7"
                android:text="Medium Text"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textIsSelectable="true" />


        </LinearLayout>


        <LinearLayout
            android:id="@+id/PortDetail4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/PortDetail3"
            android:layout_centerHorizontal="true"
            android:orientation="horizontal"

            android:weightSum="1">

            <TextView
                android:id="@+id/portMacAddressID1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="20sp"
                android:layout_weight="0.3"
                android:text="MAC Address"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/portMacAddressID"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20sp"
                android:layout_weight="0.7"
                android:text="Medium Text"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textIsSelectable="true" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/PortDetail5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/PortDetail4"
            android:layout_centerHorizontal="true"
            android:orientation="horizontal"

            android:weightSum="1">

            <TextView
                android:id="@+id/portStatus1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="20sp"
                android:layout_weight="0.3"
                android:text="Status"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/portStatus"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20sp"
                android:layout_weight="0.7"
                android:text="Medium Text"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textIsSelectable="true" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/PortDetail6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/PortDetail5"
            android:layout_centerHorizontal="true"
            android:orientation="horizontal"

            android:weightSum="1">

            <TextView
                android:id="@+id/portAdminState1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="20sp"
                android:layout_weight="0.3"
                android:text="Admin State"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/portAdminState"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20sp"
                android:layout_weight="0.7"
                android:text="Medium Text"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textIsSelectable="true" />


        </LinearLayout>


        <LinearLayout
            android:id="@+id/PortDetail8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/PortDetail6"
            android:layout_centerHorizontal="true"
            android:orientation="horizontal"

            android:weightSum="1">

            <TextView
                android:id="@+id/portCreated1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="20sp"
                android:layout_weight="0.3"
                android:text="Created Time"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/portCreated"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20sp"
                android:layout_weight="0.7"
                android:text="Medium Text"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textIsSelectable="true" />


        </LinearLayout>

    </RelativeLayout>

</ScrollView>